	Eid = {L29},
	Eprint = {1404.4872},
	Journal = {\apjl},
	Keywords = {galaxies: evolution, galaxies: fundamental parameters, galaxies: high-redshift, galaxies: structure},
	Month = jun,
	Pages = {L29},
	Title = {{MOSFIRE Absorption Line Spectroscopy of z $\gt$ 2 Quiescent Galaxies: Probing a Period of Rapid Size Growth}},
	Volume = 788,
	Year = 2014,
